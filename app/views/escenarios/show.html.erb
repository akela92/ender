<% provide(:title, 'Comprobar resultado') %>
<h1>Comprobar resultado</h1>
<%= form_for(@escenario, html: { multipart: true }) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
<span class="field">
    <%= f.file_field :fichero, accept: 'text/html' %>
  </span>
  <%= f.submit "Comprobar resultado", class: "btn btn-primary" %>
  
<% end %>

<%= link_to 'Edit', edit_escenario_path(@escenario) %> |
<%= link_to 'Back', escenarios_path %>
