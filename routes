                        PUT    /password_resets/:id(.:format)          password_resets#update
             microposts POST   /microposts(.:format)                   microposts#create
              micropost DELETE /microposts/:id(.:format)               microposts#destroy
          relationships POST   /relationships(.:format)                relationships#create
           relationship DELETE /relationships/:id(.:format)            relationships#destroy
               maquinas GET    /maquinas(.:format)                     maquinas#index
                        POST   /maquinas(.:format)                     maquinas#create
            new_maquina GET    /maquinas/new(.:format)                 maquinas#new
           edit_maquina GET    /maquinas/:id/edit(.:format)            maquinas#edit
                maquina GET    /maquinas/:id(.:format)                 maquinas#show
                        PATCH  /maquinas/:id(.:format)                 maquinas#update
                        PUT    /maquinas/:id(.:format)                 maquinas#update
                        DELETE /maquinas/:id(.:format)                 maquinas#destroy
